<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error" name="PropertiesConfig">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="[%-5level] %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="base" level="info" additivity="false">
			<AppenderRef ref="STDOUT" />
		</Logger>
		<Root level="info">
			<AppenderRef ref="STDOUT" />
		</Root>
	</Loggers>
</Configuration>